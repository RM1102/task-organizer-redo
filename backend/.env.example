
# App
PORT=3000

# Supabase (Get these from your Supabase Project Settings)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-service-role-key

# Google Calendar (Get these from Google Cloud Console)
# Redirect URI: http://localhost:3000/calendar/callback/google
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:3000/calendar/callback/google

# Microsoft Graph (Get these from Azure Portal)
# Redirect URI: http://localhost:3000/calendar/callback/microsoft
MICROSOFT_CLIENT_ID=
MICROSOFT_CLIENT_SECRET=
MICROSOFT_REDIRECT_URI=http://localhost:3000/calendar/callback/microsoft
